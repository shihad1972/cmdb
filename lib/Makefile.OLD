CFLAGS = -ansi -std=c99 -pedantic -O0 -W -Wall -Wconversion -Wshadow -Wcast-qual -Wwrite-strings -Wunused -g -D_POSIX_C_SOURCE=200809L
CPPFLAGS += -I../src/ -I../include/
lib_LIBRARIES = libailsacmdb.a

#libailsacmdb.a:	libailsacmdb.a(b64.o) libailsacmdb.a(ailsacmdb.o) libailsacmdb.a(parse_config.o)
libailsacmdb.a:	libailsacmdb.a(ailsacmdb.o) libailsacmdb.a(parse_config.o) libailsacmdb.a(logging.o)

libailsacmdb.a(b64.o):	b64.o
	$(AR) $(ARFLAGS) $@ $<

libailsacmdb.a(ailsacmdb.o):	ailsacmdb.o
	$(AR) $(ARFLAGS) $@ $<

libailsacmdb.a(parse_config.o):	parse_config.o
	$(AR) $(ARFLAGS) $@ $<

libailsacmdb.a(logging.o):	logging.o
	$(AR) $(ARFLAGS) $@ $<
.PHONY:	clean

clean:
	$(RM) *.o *.a
