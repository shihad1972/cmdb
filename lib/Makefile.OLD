CFLAGS = -ansi -std=c99 -pedantic -O0 -W -Wall -Wconversion -Wshadow -Wcast-qual -Wwrite-strings -Wunused -g -D_POSIX_C_SOURCE=200809L
CPPFLAGS = -I../src/ -I../include/ -I/usr/include/mysql
lib_LIBRARIES = libailsacmdb.a libailsasql.a

#libailsacmdb.a:	libailsacmdb.a(b64.o) libailsacmdb.a(ailsacmdb.o) libailsacmdb.a(parse_config.o)
libailsacmdb.a:	libailsacmdb.a(ailsacmdb.o) libailsacmdb.a(parse_config.o) \
		libailsacmdb.a(logging.o) libailsacmdb.a(networking.o) libailsacmdb.a(errors.o)\
		libailsacmdb.a(regexp.o) libailsacmdb.a(fileio.o) libailsacmdb.a(data.o) \
		libailsacmdb.a(list.o) libailsacmdb.a(hash.o) \
		libailsacmdb.a(cmdb_data.o) libailsacmdb.a(cbc_data.o) libailsacmdb.a(dnsa_data.o)

libailsasql.a:	libailsasql.a(sql.o) \

libailsacmdb.a(b64.o):	b64.o
	$(AR) $(ARFLAGS) $@ $<

libailsacmdb.a(ailsacmdb.o):	ailsacmdb.o
	$(AR) $(ARFLAGS) $@ $<

